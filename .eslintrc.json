{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": [
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint", 
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error", {
            "semi": false,
            "singleQuote": true
        }]
    }
}
